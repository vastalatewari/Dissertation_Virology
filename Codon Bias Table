install.packages(remotes)
remotes::install_github("alex-sbu/CPBias")
 sequences <- c("", 
               "", 
               "",
               "")
CPBtable(sequences, dnfControl = FALSE, transTable = standardTranslation,
         name = TRUE, save = TRUE, location = NULL, silent = FALSE)
ccdsCPB <- CPBtable(sequences)
ccds.sample <- ccdsCPB[[1]]
wsn <- ("")
wsnCPS <- CPScalc(wsn, ccds.sample)
UA<- ("")
both <- ("")
CG<- ("")
CGCPS <- CPScalc(CG, ccds.sample)
UACPS <-CPScalc(UA, ccds.sample)
bothCPS <-CPScalc(both, ccds.sample)
